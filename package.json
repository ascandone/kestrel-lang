    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.1",
    "eslint": "^8.56.0",
    "ohm-js": "^17.1.0",
    "prettier": "^3.2.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "vscode-languageserver-textdocument": "^1.0.11"
